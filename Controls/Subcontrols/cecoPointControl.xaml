<Thumb x:Class="Celery.Controls.Subcontrols.cecoPointControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Celery.Controls.Subcontrols"
             mc:Ignorable="d" 
             d:DesignHeight="11" d:DesignWidth="11" DragDelta="Thumb_DragDelta" DragStarted="Thumb_DragStarted" DragCompleted="Thumb_DragCompleted">
    <Thumb.Template>
        <ControlTemplate>
            <Ellipse Fill="#FFE3F6FF"
                     HorizontalAlignment="Left" Height="11" Stroke="Black"
                     VerticalAlignment="Top"
                     Width="11"
                     StrokeThickness="1"
                     Opacity="0.85"/>
        </ControlTemplate>
    </Thumb.Template>
</Thumb>
